generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User{
  id String @id @default(uuid())
  username String
  profilePicture String?
}

enum ModelTypeEnum{
Man
Woman
Others
}
enum EthinictyEnum{
  White
  Black
  AsianAmerican
  EastAsian
  SouthEastAsian
  SouthAsianMiddle
   Eastern
   Pacific
  Hispanic
}
enum EyeColorEnum{
  Brown
  Blue
  Hazel
  Gray
}
model Model{
id String @id @default(uuid())
name String
type ModelTypeEnum
age Int
bald Boolean
ethinicity EthinictyEnum
eyecolor EyeColorEnum

}
model TrainingImages{
  id String @id @default(uuid())
  imagesUrl String
  modelId String
}
model OutputImages{
  id String @id @default(uuid())
  imageUrl String
  modelId String
  
}